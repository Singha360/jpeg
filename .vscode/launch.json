{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "(gdb) Launch",
			"type": "cppdbg",
			"request": "launch",
			// Resolved by CMake Tools:
			"program": "${command:cmake.launchTargetPath}",
			"args": [
				"./samples/encImg2.jpg"
			],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [
				{
					"name": "PATH",
					"value": "$PATH:$HOME/some_path"
				},
				{
					"name": "OTHER_VALUE",
					"value": "Something something"
				}
			],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			]
		}
	]
}